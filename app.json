{
    "name": "Simple Userbot",
    "description": "A Telethon based Telegram Userbot.",
    "buildpacks": [{"url": "heroku/python"}],
    "env": {
        "BOT_TOKEN": {
            "description": "Your bot token. Get one from @BotFather duh",
            "value": "",
        },
        "BOT_USERNAME": {
            "description": "Your bot username with @",
            "value": "",
        },
        "API_ID": {
            "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
            "value": "",
        },
        "API_HASH": {
            "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
            "required": true,
            "value": "",
        },
        "SIMPLE_SESSION": {
            "description": "Telegram Telethon STRING SESSION",
            "required": true,
            "value": "",
        },
        "LOGGER_ID": {
            "description": "CHAT_ID int of log chat",
            "required": false,
            "value": "-100",
        }
    },
    "keywords": [
        "telegram",
        "Telethon",
        "Simple Userbot",
    ],
}
